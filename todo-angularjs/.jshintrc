{
  "node": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "laxcomma": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "white": true,
  "globals": {
    "angular": false,
    "console": false,
    "jQuery": false
  },
  "maxparams": 5,
  "maxdepth": 3,
  "maxcomplexity": 3,
  "maxlen": 130,
  "maxstatements": 25
}
